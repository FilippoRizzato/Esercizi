#include <stdio.h>
 
int main()
{
  int num1;
        int num2;
        double risultato;
    do{
 
       
        printf("inserisci il 1° numero\n");
        printf("Inserisci il 2° numero \n");
        scanf("%d", &num1);
        scanf("%d", &num2);
 
        if(num2 < num1)
        {
            risultato = (double)num2/(double)num1;
 
        }
        else{
            risultato = (double)num1/(double)num2;
        }
        printf("\n\t risulta %f\n\n", risultato);
 
 
    }while(num1 != 0 && num2 != 0);
    return 0;
    }

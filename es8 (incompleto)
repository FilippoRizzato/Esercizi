#include <stdio.h>
#include <math.h>

int main(){
int num1, n;
double somma, rad;
int conta = 0;
do{

printf("Inserisci n\n");
scanf("%d", &n);
}while(n < 0);
do{
   printf("Inserisci num1\n");
   scanf("%d", &num1);
   conta++;
    if (num1 >= 0)
    {
     rad = sqrt(num1);
     somma = somma+rad;
     }
     else
         printf("Non posso fare la radice \n");
}while(conta <= n && num1 > 0);
printf("La somma di radici Ã¨:%f\n\n", somma);

return 0;
}
